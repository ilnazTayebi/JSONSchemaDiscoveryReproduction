  "license": "MIT",
  "author": "Felipe de Souza da Costa",
  "description": "Web application for extraction of documents nosql database schemas",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "node dist/server/app.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "postinstall": "tsc -p server && ng build --aot -prod"
  },
